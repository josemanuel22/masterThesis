@entry{closure,
 name={\ensuremath{\overline{E}}},
 description={topological closure of \ensuremath{E}}
}
@entry{boundary,
 name={\ensuremath{\partial E}},
 description={topological boundary of \ensuremath{E}}
}
@entry{strictlycompact,
 name={\ensuremath{E \subset \subset F}},
 description={\ensuremath{\overline{E}\subset F}, \ensuremath{\overline{E}} compact}
}
@entry{ball,
 name={\ensuremath{B_{\varrho}(x)}},
 description={open ball with center \ensuremath{x} and radius \ensuremath{\varrho}}
}
@entry{distsetpoint,
 name={\ensuremath{\dist{x}{E}}},
 description={infimum of \ensuremath{d(x,y)} as \ensuremath{y} varies in \ensuremath{E}}
}
@entry{dilatation,
  name={\ensuremath{I_{\varrho}(E)}},
  description={open \ensuremath{\varrho} neighbourhood \ensuremath{\{x\in X : dist(x,E)< \varrho\}}}
}
@entry{distsetset,
  name={\ensuremath{dist(E,F)}},
  description={infimum of \ensuremath{d(x,y)} as \ensuremath{x} varies in \ensuremath{E} and \ensuremath{y} varies in \ensuremath{F}}
}
